{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "node: serve",
        "path": "/commercialoperator/frontend/commercialoperator",
        "options": {
          "env": {
            "PORT": "9107"
          }
        },
        "type": "npm",
        "script": "serve",
        "isBackground": true,
        "problemMatcher": [
          {
            "pattern": {
              "regexp": "^([^\\s].*)[\\(:](\\d+)[,:](\\d+)(?:\\):\\s+|\\s+-\\s+)(error|warning|info)\\s+TS(\\d+)\\s*:\\s*(.*)$",
              "file": 1,
              "line": 2,
              "column": 3,
              "severity": 4,
              "code": 5,
              "message": 6
            },
            "background": {
              "activeOnStart": true,
              "beginsPattern": "Starting development server.",
              "endsPattern": "Compiled successfully."
            }
          }
        ]
      }
    ]
  }
